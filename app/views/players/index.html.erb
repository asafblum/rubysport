<h1>Listing teams</h1>

<table>
  <tr>
    <th>Nick</th>
    <th>Blogurl</th>
    <th>Members count</th>
    <th colspan="3">Actions</th>
    
  </tr>

<% Player.all.each do |player| %>
  <tr>
    <td><%= player.email %></td>
     <td><%= link_to player.nick, player%></td>
  </tr>
<% end %>
</table>

